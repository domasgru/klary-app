{
  "name": "fbapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/composition-api": "^1.0.0-beta.14",
    "@vuelidate/core": "^2.0.0-alpha.7",
    "@vuelidate/validators": "^2.0.0-alpha.5",
    "animejs": "^3.2.0",
    "array-sort": "^1.0.0",
    "autosize": "^4.0.2",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.30",
    "fast-array-diff": "^0.2.0",
    "firebase": "^7.14.2",
    "fuse.js": "^6.4.1",
    "get-value": "^3.0.1",
    "lodash.difference": "^4.5.0",
    "lottie-player-vue": "0.0.14",
    "lottie-vuejs": "0.3.5",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "shortid": "^2.2.15",
    "svg-sprite-loader": "^5.0.0",
    "v-click-outside": "^3.0.1",
    "vue": "^3.0.2",
    "vue-click-outside": "^1.1.0",
    "vue-fuse": "^2.2.1",
    "vue-router": "^4.0.0-rc.1",
    "vue-shortkey": "^3.1.7",
    "vuefire": "^2.2.2",
    "vuelidate": "^0.7.5",
    "vuex": "^4.0.0-rc.1",
    "vuexfire": "^3.2.4"
  },
  "devDependencies": {
    "@babel/parser": "^7.7.5",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "^4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "stylelint": "^13.3.3",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "svgo": "^1.1.0",
    "svgo-loader": "^2.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ],
          "extensions": [
            ".js",
            ".vue"
          ]
        }
      }
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "no-use-before-define": 0,
      "no-debugger": 0,
      "linebreak-style": 0,
      "eslint linebreak-style": [
        0,
        "error",
        "windows"
      ],
      "global-require": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-unused-vars": 0,
      "template-curly-spacing": "off",
      "indent": "off",
      "max-len": [
        "error",
        {
          "code": 140,
          "ignoreStrings": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
