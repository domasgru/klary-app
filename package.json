{
  "name": "fbapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "animejs": "^3.2.0",
    "autosize": "^4.0.2",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.30",
    "fast-array-diff": "^0.2.0",
    "firebase": "^7.14.2",
    "fuse.js": "^6.4.1",
    "lodash.difference": "^4.5.0",
    "lottie-player-vue": "0.0.14",
    "lottie-vuejs": "0.3.5",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "shortid": "^2.2.15",
    "svg-sprite-loader": "^5.0.0",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-fuse": "^2.2.1",
    "vue-router": "^3.1.6",
    "vuefire": "^2.2.2",
    "vuelidate": "^0.7.5",
    "vuex": "^3.3.0",
    "vuexfire": "^3.2.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.7.5",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "svgo": "^1.1.0",
    "svgo-loader": "^2.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ],
          "extensions": [
            ".js",
            ".vue"
          ]
        }
      }
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "linebreak-style": 0,
      "eslint linebreak-style": [
        0,
        "error",
        "windows"
      ],
      "global-require": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-debugger": 0,
      "no-unused-vars": 0,
      "template-curly-spacing": "off",
      "indent": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
